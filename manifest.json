{
  "manifest_version": 3,
  "name": "LinkedIn to Google Contacts",
  "version": "0.1.0",
  "description": "Link your LinkedIn contacts to Google Contacts",
  "permissions": [
    "sidePanel"
  ],
  "action": {
    "default_title": "Open LinkedIn to Google Contacts"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}

